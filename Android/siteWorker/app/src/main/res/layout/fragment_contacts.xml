                        <TextView
                            android:layout_marginLeft="5dp"
                            android:id="@+id/contacts_textView11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/fragment_contacts_ec_mobile"
